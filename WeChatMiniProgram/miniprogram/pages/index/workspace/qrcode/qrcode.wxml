<!--pages/index/workspace/qrcode/qrcode.wxml-->
<view class="container">
    <label class="title"> Access QR Code</label>
    <canvas class='canvas' style="width:{{qrcodeWidth}}px; height:{{qrcodeWidth}}px;" canvas-id='canvas'></canvas>
    <text style="position: relative; left: 0rpx; top: 186rpx; width: 497rpx; height: 122rpx; display: block; box-sizing: border-box">Current Time: \n {{time}}</text>
    <text style="position: relative; left: -15rpx; top: 186rpx; width: 467rpx; height: 94rpx; display: block; box-sizing: border-box">Application Status: \n {{status}}</text>
</view>



